<%- include('../partials/header.ejs') %>
<div class="register">
  <form action="/account/login" method="post">
    <div>
      <h3>Masuk Sekarang</h3>
    </div>
    <div class="mb-2">
      <input type="email" name="email" class="text_field" placeholder="Email" />
    </div>
    <div class="mb-2">
      <input
        type="password"
        name="password"
        class="text_field"
        placeholder="Sandi"
      />
    </div>
    <div>
      <input class="btn btn-danger btn-sm" type="submit" value="Login" />
      <div>Belum punya akun?</div>
      <a href="/account/register">Register</a>
    </div>
    <a href="/">Back</a>
  </form>
</div>
